{
  "mcpServers": {
    "chimech": {
      "command": "node",
      "args": ["./dist/index.js"],
      "cwd": "/path/to/chimechmcp",
      "env": {
        "CHIMECH_API_KEY": "your-api-key-here",
        "CHIMECH_SERVER_URL": "https://api.chimech.com",
        "LOG_LEVEL": "info",
        "CACHE_TTL": "300",
        "REQUEST_TIMEOUT": "30000",
        "RETRY_COUNT": "3",
        "MAX_CONCURRENT_REQUESTS": "5"
      }
    }
  }
} 